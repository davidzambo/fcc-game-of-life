  "dependencies": {
    "font-awesome": "github:FortAwesome/Font-Awesome",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "build-css": "sass src/styles/scss/styles.scss:src/styles/css/styles.css",
    "watch-css": "npm run build-css && sass --watch src/styles/scss/styles.scss:src/styles/css/styles.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
